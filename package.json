{
  "name": "@auroratide/svelte-custom-element-transitions",
  "version": "0.0.1",
  "description": "Utility for making svelte transitions work on custom elements",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "example": "rollup -c examples/rollup.config.js -w",
